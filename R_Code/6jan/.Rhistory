{
a<-c(1,2,3,4,5)
b<-c(1,2,3,4,5)
result<-TRUE
if(length(a)==length(b)){
for(x in a == b){
result<-x
if(!x){
break
}
}
}else{
result<-FALSE
}
if(result){
print('Equal')
}else{
print('Not Equal')
}
}
